  },
  "devDependencies": {
    "chakram": "1.5.0",
    "bluebird": "3.5.1",
    "codeclimate-test-reporter": "0.x.x",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-plugin-import": "^2.8.0",
